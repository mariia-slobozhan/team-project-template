 {{#each this}}
<div class="photo-card">
  <img src="{{webformatURL}}" alt="{{tags}}" class="picture" data-src = {{largeImageURL}} />

  <div class="stats">
    <p class="stats-item">
      <i class="material-icons md-18">thumb_up</i>
     <span class="quantity">{{likes}}</span> 
    </p>
    <p class="stats-item">
      <i class="material-icons md-18">visibility</i>
     <span class="quantity">{{views}}</span> 
    </p>
    <p class="stats-item">
      <i class="material-icons md-18">comment</i>
     <span class="quantity">{{comments}}</span>
    </p>
    <p class="stats-item">
      <i class="material-icons md-18">cloud_download</i>
      <span class="quantity">{{downloads}}</span>
    </p>
  </div>
</div>
 {{/each}}